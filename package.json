{
  "name": "express-auth-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "nodemon",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json}\"",
    "db:dev:rm": "docker compose down -v",
    "db:dev:up": "docker-compose up -d --build",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js ",
    "typeorm:generate-migration": "npm run typeorm -- migration:generate src/db/migrations/$npm_config_name -d src/db/appDataSource.ts",
    "typeorm:run-migrations": "npm run typeorm -- migration:run -d src/db/appDataSource",
    "test": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest --runInBand",
    "test:unit": "npm run test -- --testPathPattern=unit",
    "test:integration": "npm run test -- --testPathPattern=integration",
    "test:e2e": "npm run test -- --testPathPattern=e2e",
    "test:coverage": "jest --coverage --runInBand --testPathPattern=unit",
    "typeorm:prod:run-migrations": "npm run typeorm -- migration:run -d dist/src/db/appDataSource.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gevorgabgaryan/Express-Auth-app.git"
  },
  "author": "Gevorg Abgaryan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gevorgabgaryan/Express-Auth-app/issues"
  },
  "homepage": "https://github.com/gevorgabgaryan/Express-Auth-app#readme",
  "dependencies": {
    "@nartc/automapper": "^7.0.4",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.43.0",
    "@opentelemetry/sdk-metrics": "^1.22.0",
    "@opentelemetry/sdk-node": "^0.49.1",
    "@opentelemetry/sdk-trace-node": "^1.22.0",
    "@sendgrid/mail": "^8.1.0",
    "amqplib": "^0.10.3",
    "argon2": "^0.31.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jimp": "^0.22.12",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "routing-controllers": "^0.10.4",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.7",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/passport": "^1.0.16",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
